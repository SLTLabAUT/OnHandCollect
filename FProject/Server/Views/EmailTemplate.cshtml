                font-weight: var(--fontWeight-Regular);
            }


        .ms-Button.ms-Button--commandBar {
            min-width: 40px;
            padding: 0 4px;
            height: 40px;
            color: var(--palette-NeutralPrimary);
            background-color: var(--palette-White);
            border: none;
            border-radius: 0;
        }

            .ms-Button.ms-Button--commandBar:hover {
                color: var(--palette-NeutralDark);
                background-color: var(--palette-NeutralLighter);
            }

                .ms-Button.ms-Button--commandBar:hover .ms-Button .ms-Button-icon {
                    color: var(--palette-ThemeDarkAlt);
                }

                .ms-Button.ms-Button--commandBar:hover .ms-Button .ms-Button-menuIcon {
                    color: var(--palette-NeutralPrimary);
                }

            .ms-Button.ms-Button--commandBar:active {
                color: var(--semanticTextColors-Black);
                background-color: var(--palette-NeutralLight);
            }

                .ms-Button.ms-Button--commandBar:active .ms-Button .ms-Button-icon {
                    color: var(--palette-ThemeDarkAlt);
                }

                .ms-Button.ms-Button--commandBar:active .ms-Button .ms-Button-menuIcon {
                    color: var(--palette-NeutralPrimary);
                }

            .ms-Button.ms-Button--commandBar.is-checked:not(.is-disabled) {
                color: var(--Palette-NeutralDark);
                background-color: var(--palette-NeutralLight);
            }

                .ms-Button.ms-Button--commandBar.is-checked:not(.is-disabled)
                .ms-Button
                .ms-Button-icon {
                    color: var(--Palette-ThemeDark);
                }

                .ms-Button.ms-Button--commandBar.is-checked:not(.is-disabled)
                .ms-Button
                .ms-Button-menuIcon {
                    color: var(--Palette-NeutralDark);
                    background-color: var(--palette-NeutralPrimary);
                }

                .ms-Button.ms-Button--commandBar.is-checked:not(.is-disabled):hover {
                    background-color: var(--palette-NeutralQuaternaryAlt);
                }

                    .ms-Button.ms-Button--commandBar.is-checked:not(.is-disabled):hover
                    .ms-Button
                    .ms-Button-icon {
                        color: var(--Palette-ThemeDark);
                    }

                    .ms-Button.ms-Button--commandBar.is-checked:not(.is-disabled):hover
                    .ms-Button
                    .ms-Button-menuIcon {
                        color: var(--Palette-NeutralDark);
                        background-color: var(--palette-NeutralPrimary);
                    }

            .ms-Button.ms-Button--commandBar.is-expanded {
                background-color: var(--palette-NeutralLight);
                color: var(--palette-NeutralDark);
            }

                .ms-Button.ms-Button--commandBar.is-expanded .ms-Button .ms-Button-icon {
                    color: var(--palette-ThemeDark);
                }

                .ms-Button.ms-Button--commandBar.is-expanded .ms-Button .ms-Button-menuIcon {
                    color: var(--palette-NeutralPrimary);
                }

            .ms-Button.ms-Button--commandBar.is-disabled {
                background-color: var(--palette-White);
            }

                .ms-Button.ms-Button--commandBar.is-disabled .ms-Button .ms-Button-icon {
                    color: var(--semanticTextColors-DisabledBodySubtext);
                }

            .ms-Button.ms-Button--commandBar .ms-Button .ms-Button-icon {
                color: var(--palette-ThemePrimary);
            }

            .ms-Button.ms-Button--commandBar .ms-Button .ms-Button-label {
                font-weight: normal;
            }

            .ms-Button.ms-Button--commandBar .ms-Button .ms-Button-menuIcon {
                color: var(--palette-NeutralSecondary);
            }


        .ms-Button--icon {
            padding: 0 4px;
            min-width: 32px;
            height: 32px;
            background-color: transparent;
            border: none;
            color: var(--semanticTextColors-Link);
        }

            .ms-Button--icon:hover {
                color: var(--palette-ThemeDarkAlt);
                background-color: var(--palette-NeutralLighter);
            }

            .ms-Button--icon:active {
                color: var(--palette-ThemeDark);
                background-color: var(--palette-NeutralLight);
            }

            .ms-Button--icon.is-checked:not(.is-disabled) {
                color: var(--palette-ThemeDark);
                background-color: var(--palette-NeutralLight);
                border: none;
            }

            .ms-Button--icon.is-checked:hover {
                background-color: var(--palette-NeutralQuaternaryAlt);
                color: var(--palette-ThemeDark);
            }

            .ms-Button--icon.is-disabled {
                border-color: var(--palette-NeutralQuaternaryAlt);
            }


        .ms-Button.ms-Button--command {
            min-width: unset;
            padding: 0px 4px;
            height: 40px;
            color: var(--palette-NeutralPrimary);
            background-color: transparent;
            border: 1px solid transparent;
        }

            .ms-Button.ms-Button--command:hover {
                color: var(--palette-ThemePrimary);
            }

            .ms-Button.ms-Button--command.is-expanded {
                color: var(--palette-ThemePrimary);
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-icon:hover {
                color: var(--palette-ThemePrimary);
            }

            .ms-Button.ms-Button--command:active {
                color: var(--palette-Black);
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-icon:active {
                color: var(--palette-ThemeDarker);
            }

            .ms-Button.ms-Button--command.is-disabled {
                color: var(--palette-NeutralTertiary);
                background-color: transparent;
            }

            .ms-Button.ms-Button--command.is-checked:not(.is-disabled) {
                color: var(--palette-Black);
            }

            .ms-Button.ms-Button--command.is-checked .ms-Button .ms-ButtonIcon {
                color: var(--palette-ThemeDarker);
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-flexContainer {
                justify-content: flex-start;
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-icon {
                color: var(--palette-ThemeDarkAlt);
            }

            .ms-Button.ms-Button--command.is-disabled .ms-Button .ms-Button-icon {
                color: inherit;
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-menuIcon {
                color: var(--palette-NeutralSecondary);
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-textContainer {
                flex-grow: 0;
            }

            .ms-Button.ms-Button--command .ms-Button .ms-Button-label {
                font-weight: var(--fontWeight-Regular);
            }


        .ms-Button.ms-Button--compound {
            max-width: 280px;
            min-height: 72px;
            padding: 16px 12px;
            height: auto;
        }

            .ms-Button.ms-Button--compound .ms-Button .ms-Button-flexContainer {
                flex-direction: row;
                align-items: flex-start;
                min-width: 100%;
                margin: unset;
            }

            .ms-Button.ms-Button--compound .ms-Button .ms-Button-textContainer {
                text-align: left;
            }

            .ms-Button.ms-Button--compound .ms-Button .ms-Button-label {
                margin: 0 0 5px 0;
                line-height: 100%;
                font-weight: var(--fontWeight-SemiBold);
            }

            .ms-Button.ms-Button--compound .ms-Button .ms-Button-description {
                line-height: 100%;
            }

        .ms-Button--default.ms-Button--compound .ms-Button .ms-Button-description {
            color: var(--palette-NeutralSecondary);
        }

        .ms-Button--default.ms-Button--compound:hover
        .ms-Button
        .ms-Button-description {
            color: var(--palette-NeutralDark);
        }

        .ms-Button--default.ms-Button--compound.is-checked
        .ms-Button
        .ms-Button-description {
            color: inherit;
        }

        .ms-Button--default.ms-Button--compound.is-disabled
        .ms-Button
        .ms-Button-description {
            color: inherit;
        }

        .ms-Button--primary.ms-Button--compound .ms-Button .ms-Button-description {
            color: var(--palette-White);
        }

        .ms-Button--primary.ms-Button--compound:hover
        .ms-Button
        .ms-Button-description {
            color: var(--palette-White);
        }

        .ms-Button--primary.ms-Button--compound.is-checked
        .ms-Button
        .ms-Button-description {
            color: inherit;
        }

        .ms-Button--primary.ms-Button--compound.is-disabled
        .ms-Button
        .ms-Button-description {
            color: inherit;
        }

        * {
            margin: 0;
            padding: 0;
            box-sizing: border-box;
        }

        html,
        body {
            height: 100%;
            width: 100%;
            overflow: auto;
        }

        body {
            display: flex;
            flex-direction: column;
        }

        :not(.ms-Icon) {
            font-family: IRANSans, Vazir, sans-serif !important;
        }

        .main {
            width: 50%;
            background-color: rgba(242, 242, 242, 0.9);
            padding: 2% 2%;
            text-align: center;
            display: flex;
            flex-direction: column;
            margin: 5em auto;
            flex-grow: 1;
        }

            .main .title {
                margin-bottom: 0.25em;
            }

            .main .description {
                padding: 1em;
                flex-grow: 1;
                overflow: auto;
                text-align: justify;
                text-align-last: center;
            }

                .main .description p {
                    text-align-last: right;
                }

                .main .description > :not(:first-child) {
                    margin-top: 1em;
                }

        .header {
            display: flex;
            align-items: center;
            justify-content: center;
            padding: 1em;
            background-color: rgba(242, 242, 242, 0.9);
        }

        .footer {
            display: flex;
            align-items: center;
            flex-direction: column;
            padding: 1em;
            background-color: rgba(242, 242, 242, 0.9);
        }

        hr {
            margin: 0.25em 0;
        }
    </style>
</head>
<body>
    <header class="header ms-depth-16">
        <h1>سامانه‌ی جامع جمع‌آوری دادگان دست‌نوشته‌ی برخط فارسی</h1>
    </header>
    <section class="main ms-depth-64">
        <h1 class="title">
            @Model.Title
        </h1>
        <hr />
        <div class="description">
            <p>
                کاربر گرامی،
                <br />
                @Html.Raw(Model.Description)، روی دکمه‌ی زیر کلیک کنید.
            </p>
            <a class="ms-Button ms-Button--primary" href="@Model.Uri">
                <span class="ms-Button-flexContainer">
                    <span class="ms-Button-textContainer">
                        <span class="ms-Button-label">@Model.ButtonLabel</span>
                    </span>
                </span>
            </a>
        </div>
    </section>
    <footer class="footer ms-depth-16">
        <div><a>Unsubscribe</a></div>
    </footer>
</body>
</html>

