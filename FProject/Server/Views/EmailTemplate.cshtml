        .ms-Button {
            outline: transparent;
            position: relative;
            font-size: var(--fontSize-Medium);
            font-weight: var(--fontWeight-Regular);
            box-sizing: border-box;
            border: 1px solid var(--semanticColors-ButtonBorder);
            user-select: none;
            display: inline-block;
            text-decoration: none;
            text-align: center;
            cursor: pointer;
            vertical-align: top;
            padding: 0 16px;
            min-width: 80px;
            height: 32px;
            border-radius: var(--effects-RoundedCorner2);
        }

            .ms-Button::-moz-focus-inner {
                border: 0;
            }

        .ms-Fabric--isFocusVisible .ms-Button:focus::after {
            content: "";
            position: absolute;
            left: 2px;
            top: 2px;
            bottom: 2px;
            right: 2px;
            border: 1px solid var(--palette-White);
            outline: 1px solid var(--palette-NeutralSecondary);
            z-index: var(--zIndex-FocusStyle);
        }

        .ms-Button:active > * {
            position: relative;
            left: 0;
            top: 0;
        }

        .ms-Button.is-disabled {
            background-color: var(--semanticColors-DisabledBackground);
            color: var(--semanticTextColors-DisabledText);
            cursor: default;
            pointer-events: none;
        }

        .ms-Button:hover,
        .ms-Button:focus {
            text-decoration: none;
            outline: 0;
        }

        .ms-Button:link,
        .ms-Button:visited,
        .ms-Button:active {
            text-decoration: none;
        }

        .ms-Button.ms-Button--default:not(.ms-Button--command).is-expanded {
            background-color: var(--palette-NeutralLight);
            color: var(--palette-NeutralDark);
        }

        .ms-Button.ms-Button--primary.is-expanded {
            background-color: var(--palette-ThemeDark);
            color: var(--palette-White);
        }

        .ms-Button.ms-Button--default:not(.ms-Button--command).is-expanded
        .ms-Button
        .ms-Button-icon {
            color: var(--palette-ThemeDark);
        }

        .ms-Button.ms-Button--primary.is-expanded .ms-Button .ms-Button-icon {
            color: var(--palette-White);
        }

        .ms-Button.ms-Button--default:not(.ms-Button--command).is-expanded
        .ms-Button
        .ms-Button-menuIcon {
            color: var(--palette-NeutralPrimary);
        }

        .ms-Button.ms-Button--primary.is-expanded .ms-Button .ms-Button-menuIcon {
            color: var(--palette-White);
        }

        .ms-Button.ms-Button--default:not(.ms-Button--command).is-expanded:hover {
            background-color: var(--palette-NeutralQuaternaryAlt);
        }

        .ms-Button.ms-Button--primary.is-expanded:hover {
            background-color: var(--palette-ThemeDark);
        }

        .ms-Button.is-disabled .ms-Button .ms-Button-icon {
            color: var(--semanticTextColors-DisabledText);
        }

        .ms-Button.is-disabled .ms-Button .ms-Button-menuIcon {
            color: var(--semanticTextColors-DisabledText);
        }

        .ms-Button .ms-Button-flexContainer {
            display: flex;
            height: 100%;
            flex-wrap: nowrap;
            justify-content: center;
            align-items: center;
        }

        .ms-Button .ms-Button-textContainer {
            display: block;
            flex-grow: 1;
        }

        .ms-Button .ms-Button-description {
